	const GOOGLE_CLIENT_ID = 'REPLACE ME';
	const GOOGLE_CLIENT_SECRET = 'REPLACE ME';

	public function getApplicationName()
	{
		return OAuthSettings::GOOGLE_APPLICATION_NAME;
	}

	public function getClientId()
	{
		return OAuthSettings::GOOGLE_CLIENT_ID;
	}

	public function getClientSecret()
	{
		return OAuthSettings::GOOGLE_CLIENT_SECRET;
	}
}
